{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport $ from 'jquery';\nimport router from '@/router/index';\nimport { getCurrentInstance } from 'vue';\nexport default {\n  name: \"LoginForm\",\n  data() {\n    return {\n      get_verification_code_text: \"获取验证码\"\n    };\n  },\n  setup() {\n    const datab = getCurrentInstance();\n    const accountType = () => {\n      $('.login_verification_code_module').hide();\n      $('.login_password').show();\n      $('.verification_code_type').css(\"border-bottom\", \"none\");\n      $('.account_type').css(\"border-bottom\", \"3px solid #66afe9\");\n    };\n    const verificationCodeType = () => {\n      $('.login_password').hide();\n      $('.login_verification_code_module').show();\n      $('.account_type').css(\"border-bottom\", \"none\");\n      $('.verification_code_type').css(\"border-bottom\", \"3px solid #66afe9\");\n    };\n    const login = () => {\n      router.push({\n        name: 'home'\n      });\n    };\n    const getVerificationCode = tm => {\n      // datab.data.get_verification_code_text = \"获取验证码\";\n      // datab.data.get_verification_code_text = i + \"s后重试\";\n      $('.login_get_verification_code_btn').disabled = true;\n      datab.data.get_verification_code_text = \"60s后重试\";\n      let countDown = setInterval(() => {\n        if (tm <= 50) {\n          datab.data.get_verification_code_text = \"获取验证码\";\n          clearInterval(countDown);\n        } else {\n          tm--;\n          datab.data.get_verification_code_text = tm + \"s后重试\";\n        }\n      }, 1000);\n    };\n    const forgetPassword = () => {\n      $('.login_form').hide();\n      $('.forget_password_form').show();\n    };\n    const register = () => {\n      $('.login_form').hide();\n      $(\".register_form\").show();\n    };\n    const qqLogin = () => {\n      router.push({\n        name: 'home'\n      });\n    };\n    return {\n      accountType,\n      verificationCodeType,\n      login,\n      getVerificationCode,\n      forgetPassword,\n      register,\n      qqLogin\n    };\n  }\n};","map":{"version":3,"mappings":";AAiDA,OAAOA,OAAO,QAAQ;AACtB,OAAOC,MAAK,MAAO,gBAAgB;AACnC,SAASC,kBAAiB,QAAS,KAAK;AAExC,eAAe;EACXC,IAAI,EAAE,WAAW;EACjBC,IAAI,GAAG;IACH,OAAO;MACHC,0BAA0B,EAAE;IAChC;EACJ,CAAC;EACDC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIL,kBAAkB,EAAE;IAClC,MAAMM,WAAU,GAAI,MAAM;MACtBR,CAAC,CAAC,iCAAiC,CAAC,CAACS,IAAI,EAAE;MAC3CT,CAAC,CAAC,iBAAiB,CAAC,CAACU,IAAI,EAAE;MAC3BV,CAAC,CAAC,yBAAyB,CAAC,CAACW,GAAG,CAAC,eAAe,EAAE,MAAM,CAAC;MACzDX,CAAC,CAAC,eAAe,CAAC,CAACW,GAAG,CAAC,eAAe,EAAE,mBAAmB,CAAC;IAChE,CAAC;IACD,MAAMC,oBAAmB,GAAI,MAAM;MAC/BZ,CAAC,CAAC,iBAAiB,CAAC,CAACS,IAAI,EAAE;MAC3BT,CAAC,CAAC,iCAAiC,CAAC,CAACU,IAAI,EAAE;MAC3CV,CAAC,CAAC,eAAe,CAAC,CAACW,GAAG,CAAC,eAAe,EAAE,MAAM,CAAC;MAC/CX,CAAC,CAAC,yBAAyB,CAAC,CAACW,GAAG,CAAC,eAAe,EAAE,mBAAmB,CAAC;IAC1E,CAAC;IACD,MAAME,KAAI,GAAI,MAAM;MAChBZ,MAAM,CAACa,IAAI,CAAC;QAACX,IAAI,EAAE;MAAM,CAAC,CAAC;IAC/B,CAAC;IACD,MAAMY,mBAAkB,GAAKC,EAAE,IAAK;MAChC;MACA;MACAhB,CAAC,CAAC,kCAAkC,CAAC,CAACiB,QAAO,GAAI,IAAI;MACrDV,KAAK,CAACH,IAAI,CAACC,0BAAyB,GAAI,QAAQ;MAChD,IAAIa,SAAQ,GAAIC,WAAW,CAAC,MAAM;QAC9B,IAAIH,EAAC,IAAK,EAAE,EAAE;UACVT,KAAK,CAACH,IAAI,CAACC,0BAAyB,GAAI,OAAO;UAC/Ce,aAAa,CAACF,SAAS,CAAC;QAC5B,OAAO;UACHF,EAAC,EAAG;UACJT,KAAK,CAACH,IAAI,CAACC,0BAAyB,GAAIW,EAAC,GAAI,MAAM;QACvD;MACJ,CAAC,EAAE,IAAI,CAAC;IACZ,CAAC;IACD,MAAMK,cAAa,GAAI,MAAM;MACzBrB,CAAC,CAAC,aAAa,CAAC,CAACS,IAAI,EAAE;MACvBT,CAAC,CAAC,uBAAuB,CAAC,CAACU,IAAI,EAAE;IACrC,CAAC;IACD,MAAMY,QAAO,GAAI,MAAM;MACnBtB,CAAC,CAAC,aAAa,CAAC,CAACS,IAAI,EAAE;MACvBT,CAAC,CAAC,gBAAgB,CAAC,CAACU,IAAI,EAAE;IAC9B,CAAC;IACD,MAAMa,OAAM,GAAI,MAAM;MAClBtB,MAAM,CAACa,IAAI,CAAC;QAACX,IAAI,EAAE;MAAM,CAAC,CAAC;IAC/B,CAAC;IACD,OAAO;MACHK,WAAW;MACXI,oBAAoB;MACpBC,KAAK;MACLE,mBAAmB;MACnBM,cAAc;MACdC,QAAQ;MACRC;IACJ;EACJ;AACJ","names":["$","router","getCurrentInstance","name","data","get_verification_code_text","setup","datab","accountType","hide","show","css","verificationCodeType","login","push","getVerificationCode","tm","disabled","countDown","setInterval","clearInterval","forgetPassword","register","qqLogin"],"sourceRoot":"","sources":["C:\\Users\\86178\\Desktop\\OnlineSocial\\font-end\\onlinesocial\\src\\components\\LoginForm.vue"],"sourcesContent":["<template>\n    <form class=\"login_form\">\n        <div class=\"login_select\">\n            <a href=\"javascript:void(0)\" class=\"login_select_type account_type\" @click=\"accountType()\">密码登录</a>\n            <a href=\"javascript:void(0)\" class=\"login_select_type verification_code_type\" @click=\"verificationCodeType()\">验证码登录</a>\n        </div>\n        <div class=\"login_body\">\n            <div class=\"inputBox login_phone_number\">\n                <input type=\"text\" required=\"required\">\n                <span>手机号</span>\n            </div>\n            <div class=\"inputBox login_password\">\n                <input type=\"password\" required=\"required\">\n                <span>密码</span>\n            </div>\n            <div class=\"login_verification_code_module\">\n                <div class=\"inputBox login_verification_code\">\n                    <input type=\"text\" required=\"required\">\n                    <span>验证码</span>\n                </div>\n                <div class=\"inputBox login_get_verification_code\">\n                    <input type=\"button\" v-model=\"get_verification_code_text\" class=\"btn login_get_verification_code_btn\" @click=\"getVerificationCode(60)\">\n                </div>\n            </div>\n            <div class=\"errormsg login_errormsg\">账号或密码错误</div>\n        </div>\n        <div class=\"login_footer\">\n            <button class=\"btn login_confirm_btn\" @click=\"login()\">登录</button>\n            <div class=\"forgetpassword_register\">\n                <div class=\"forget_password\">\n                    <button class=\"btn btn_link forget_password_btn\" @click=\"forgetPassword()\">忘记密码</button>\n                </div>\n                <div class=\"register\">\n                    <button class=\"btn btn_link register_btn\" @click=\"register()\">注册</button>\n                </div>\n            </div>\n            <div class=\"login_quickly\">\n                <div>\n                    <img class=\"btn login_qq_image\" src=\"../assets/qq.jpg\" alt=\"QQ图标\" @click=\"qqLogin()\">\n                </div>\n                <div class=\"login_quickly_title\">\n                    <a class=\"btn_link\">&nbsp;&nbsp;快速登录</a>\n                </div>\n            </div>\n        </div>\n    </form>\n</template>\n\n<script>\nimport $ from 'jquery';\nimport router from '@/router/index';\nimport { getCurrentInstance } from 'vue';\n\nexport default {\n    name: \"LoginForm\",\n    data() {\n        return {\n            get_verification_code_text: \"获取验证码\",\n        }\n    },\n    setup() {\n        const datab = getCurrentInstance();\n        const accountType = () => {\n            $('.login_verification_code_module').hide();\n            $('.login_password').show();\n            $('.verification_code_type').css(\"border-bottom\", \"none\");\n            $('.account_type').css(\"border-bottom\", \"3px solid #66afe9\");\n        };\n        const verificationCodeType = () => {\n            $('.login_password').hide();\n            $('.login_verification_code_module').show();\n            $('.account_type').css(\"border-bottom\", \"none\");\n            $('.verification_code_type').css(\"border-bottom\", \"3px solid #66afe9\");\n        };\n        const login = () => {\n            router.push({name: 'home'});\n        };\n        const getVerificationCode = (tm) => {\n            // datab.data.get_verification_code_text = \"获取验证码\";\n            // datab.data.get_verification_code_text = i + \"s后重试\";\n            $('.login_get_verification_code_btn').disabled = true;\n            datab.data.get_verification_code_text = \"60s后重试\";\n            let countDown = setInterval(() => {\n                if (tm <= 50) {\n                    datab.data.get_verification_code_text = \"获取验证码\";\n                    clearInterval(countDown);\n                } else {\n                    tm -- ;\n                    datab.data.get_verification_code_text = tm + \"s后重试\";\n                }\n            }, 1000);\n        };\n        const forgetPassword = () => {\n            $('.login_form').hide();\n            $('.forget_password_form').show();\n        };\n        const register = () => {\n            $('.login_form').hide();\n            $(\".register_form\").show();\n        };\n        const qqLogin = () => {\n            router.push({name: 'home'});\n        };\n        return {\n            accountType,\n            verificationCodeType,\n            login,\n            getVerificationCode,\n            forgetPassword,\n            register,\n            qqLogin,\n        }\n    }\n}\n\n</script>\n\n<style scoped>\n.login_form\n{\n    padding: 10px;\n}\n.login_select\n{\n    padding: 0 10px 0 10px;\n    display: flex;\n    justify-content: space-around;\n}\n.login_select_type\n{\n    width: 50%;\n    color: lightblue;\n    text-align: center;\n    text-decoration: none;\n}\n.account_type\n{\n    border-bottom: 3px solid #66afe9;\n}\n.login_body\n{\n    padding: 10px;\n}\n.login_footer\n{\n    padding: 0px 10px 10px 10px;\n}\n.login_confirm_btn\n{\n    width: 100%;\n    height: 35px;\n    border-radius: 5px;\n    background-color: #66afe9;\n}\n.forgetpassword_register\n{\n    display: inline-block;\n    width: 100%;\n}\n.forget_password\n{\n    float: left;\n}\n.register\n{\n    float: right;\n}\n.login_qq_image\n{\n    width: 26px;\n    height: 21px;\n    vertical-align: middle;\n}\n.login_phone_number\n{\n    margin-top: 10px;\n}\n.login_password\n{\n    margin-top: 20px;\n    margin-bottom: 4px;\n}\n.login_verification_code_module\n{\n    position: relative;\n    width: 100%;\n    margin-top: 20px;\n    display: inline-block;\n}\n.login_verification_code\n{\n    width: 50%;\n    float: left;\n}\n.login_password input:valid ~ span, \n.login_password input:focus ~ span \n{\n    border-radius: 2px;\n    background: #00dfc4;\n    color: #1d2b3a;\n}\n.login_verification_code input:valid ~ span, \n.login_verification_code input:focus ~ span \n{\n    border-radius: 2px;\n    background: #00dfc4;\n    color: #1d2b3a;\n}\n.login_get_verification_code\n{\n    width: 40%;\n    float: right;\n}\n.login_get_verification_code_btn\n{\n    width: 100%;\n}\n.login_quickly\n{\n    text-align: center;\n}\n.login_quickly_title\n{\n    margin-top: 5px;\n}\n.login_quickly_title a\n{\n    font-size: 13px;\n    user-select: none;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}